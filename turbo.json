{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**"],
			"env": [
				"CDP_WALLET_SECRET",
				"CDP_API_KEY_ID",
				"CDP_API_KEY_SECRET",
				"VERCEL_URL",
				"NETWORK"
			]
		},
		"typecheck": {
			"dependsOn": ["^typecheck"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	}
}
